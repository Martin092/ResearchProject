{
  "name" : "FS-SCB with ANOVA-F selection against Bayesian selection",

  "export_figures" : true,
  "show_figures" : true,

  "simulations" : [
    {
      "name" : "FS-SCB - bayesian selection",

      "env" : "SquareCBEnvironment",
      "learner" : "FSSquareCB",

      "trials" : 10,
      "horizon" : 1000,

      "env_config" : {
        "d" : 100,
        "sparsity":0.05,
        "noise": 0.01,
        "k": 100
      },
      "learner_config" : {
        "l_rate": 0.5,
        "strategy": "bayesian",
        "num_features": 60,
        "learn_rate": 800,
        "strat_params": {
          "gibbs_iterations": 1000,
          "M": 10,
          "s": 5,
          "warmup": 100
        }
      }
    },
    {
      "name" : "FS-SCB - anova selection",

      "env" : "SquareCBEnvironment",
      "learner" : "FSSquareCB",

      "trials" : 10,
      "horizon" : 1000,

      "env_config" : {
        "d" : 100,
        "sparsity":0.05,
        "noise": 0.01,
        "k": 100
      },
      "learner_config" : {
        "l_rate": 0.5,
        "strategy": "AnovaF",
        "num_features": 60,
        "learn_rate": 700,
        "strat_params": {
          "M": 10,
          "s": 5,
          "warmup": 100
        }
      }
    }
  ]
}